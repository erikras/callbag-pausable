{
  "name": "callbag-pausable",
  "version": "1.0.0",
  "description":
    "A callbag operator that allows values to pass through, but allows the value stream to be paused",
  "main": "dist/callbag-pausable.cjs.js",
  "jsnext:main": "dist/callbag-pausable.es.js",
  "module": "dist/callbag-pausable.es.js",
  "typings": "dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "start": "nps",
    "test": "nps test",
    "precommit": "lint-staged && npm start validate",
    "prepublish": "lint-staged && npm start validate"
  },
  "author":
    "Erik Rasmussen <rasmussenerik@gmail.com> (http://github.com/erikras)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/erikras/callbag-pausable.git"
  },
  "bugs": {
    "url": "https://github.com/erikras/callbag-pausable/issues"
  },
  "homepage": "https://github.com/erikras/callbag-pausable#readme",
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-stage-2": "^6.24.1",
    "bundlesize": "^0.17.0",
    "callbag-map": "^1.0.1",
    "callbag-mock": "^2.1.0",
    "callbag-pipe": "^1.1.1",
    "callbag-tap": "^1.0.2",
    "eslint": "^4.19.1",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-flowtype": "^2.46.3",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "lint-staged": "^7.0.4",
    "nps": "^5.9.0",
    "nps-utils": "^1.5.0",
    "prettier": "^1.12.1",
    "prettier-eslint-cli": "^4.7.1",
    "rollup": "^0.58.1",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-flow": "^1.1.1",
    "rollup-plugin-node-resolve": "^3.2.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-uglify": "^3.0.0"
  },
  "lint-staged": {
    "*.{js*,json,md,css}": ["prettier --write", "git add"]
  },
  "bundlesize": [
    {
      "path": "dist/callbag-pausable.umd.min.js",
      "threshold": "400B"
    },
    {
      "path": "dist/callbag-pausable.es.js",
      "threshold": "400B"
    },
    {
      "path": "dist/callbag-pausable.cjs.js",
      "threshold": "500B"
    }
  ],
  "dependencies": {
    "callbag-interval": "^1.0.0",
    "callbag-observe": "^1.0.0"
  }
}
